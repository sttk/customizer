{
  "camelcase" : true,
  "curly" : true,
  "eqeqeq" : true,
  "eqnull" : true,
  "esversion" : "6",
  "funcscope" : true,
  "freeze" : true,
  "globalstrict" : true,
  "indent" : 2,
  "maxdepth" : 3,
  "maxlen" : 80,
  "maxstatements" : 50,
  "newcap" : false,
  "node" : true,
  "noempty" : false,
  "quotmark" : "single",
  "strict" : true,
  "supernew" : true,
  "undef" : true,
  "unused" : true
}
